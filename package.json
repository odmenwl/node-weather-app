{
  "name": "weather-app",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "rimraf ./build && tsc && tsc-alias",
    "start:dev": "yarn nodemon",
    "start": "yarn run build && node dist/app/server.js",
    "lint": "eslint .",
    "get-weather-data": "ts-node -r tsconfig-paths/register src/weather-parser/index.ts",
    "typeorm": "ts-node --transpileOnly=true -r tsconfig-paths/register ./node_modules/typeorm/cli",
    "migration:generate": "yarn run typeorm migration:generate src/db/migrations/weather-enumm -d src/db/data-source.ts -p",
    "migration:run": "yarn run typeorm migration:run -d src/db/data-source.ts",
    "migration:revert": "yarn run typeorm migration:revert -d src/db/data-source.ts",
    "seed": "ts-node src/db/seeds/index.ts"
  },
  "dependencies": {
    "@hapi/hapi": "^21.1.0",
    "@hapi/inert": "^7.0.0",
    "@hapi/vision": "^7.0.0",
    "axios": "^1.2.1",
    "class-transformer": "^0.5.1",
    "dotenv": "^16.0.3",
    "hapi-swagger": "^15.0.0",
    "inversify": "^6.0.1",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "tsconfig-paths": "^4.1.1",
    "tslog": "^4.4.4",
    "typeorm": "^0.3.11"
  },
  "devDependencies": {
    "@types/hapi__hapi": "^20.0.13",
    "@types/hapi__inert": "^5.2.3",
    "@types/hapi__vision": "^5.5.3",
    "@types/node": "^18.11.14",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.2",
    "tscpaths": "^0.0.9",
    "typescript": "^4.9.4"
  }
}
